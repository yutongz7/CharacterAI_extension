{
    "manifest_version": 3,
    "name": "CAI Tools",
    "description": "Character.AI now has extra features. Memory Manager, cloning your chats to explore different routes, saving chats and characters!",
    "version": "2.1.0",
    "icons": {
        "128": "/icon.png"
    },
    "permissions": [],
    "background": {
        "service_worker": "/scripts/background.js"
    },
    "web_accessible_resources": [
        {
            "resources": [
                "/scripts/intercept.js",
                "/ReadOffline.html"
            ],
            "matches": [
                "https://beta.character.ai/*",
                "https://plus.character.ai/*",
                "https://character.ai/*"
            ]
        }
    ],
    "content_scripts": [
        {
            "matches": [
                "https://beta.character.ai/*",
                "https://plus.character.ai/*",
                "https://character.ai/*"
            ],
            "js": [
                "/scripts/crc32.min.js",
                "/scripts/jszip.min.js",
                "/scripts/content.js"
            ],
            "css": [
                "/style/main.css"
            ],
            "run_at": "document_end"
        }
    ]
}